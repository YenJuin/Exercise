#include<stdio.h>
#include<math.h>

double hypotenuse(double x,double y);

int main()
{
	double a,b;
	
	printf("Enter two sides of the triangle: \n");
	scanf("%d",&a);
	scanf("%d",&b);
	
	double result = hypotenuse(a,b);
	
	printf("The hypotenuse of the triangle is %d",result);
}

double hypotenuse(double x,double y)
{
	return sqrt(x*x+y*y);
}

#include<stdio.h>
#include<limits.h>

int maximum(int x,int y,int z,int w);

int main()
{
	int num1,num2,num3,num4;
	
	printf("Enter four integers: \n");
	scanf("%d%d%d%d",&num1,&num2,&num3,&num4);
	
	printf("The largest number is %d",maximum(num1,num2,num3,num4));
}

int maximum(int x,int y,int z,int w)
{
	int max;
	
	if(x>max){
		max = x;
	}
	if(y>max){
		max = y;
	}
	if(z>max){
		max = z;
	}
	if(w>max){
		max = w;
	}
}
